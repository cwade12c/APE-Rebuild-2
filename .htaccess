#FallbackResource index.php
#RewriteBase /
#RedirectMatch ^/(.*?)/$ /$1
#RewriteRule ^(.*)/$ /$1 [L,R=301]

RewriteEngine On
RewriteRule ^login /REPLACE_ME/pages/login.php [L]
RewriteRule ^logout /REPLACE_ME/pages/logout.php [L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.php [QSA,L]
